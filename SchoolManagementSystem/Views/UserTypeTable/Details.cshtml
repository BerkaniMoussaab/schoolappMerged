@model SchoolManagementSystem.UserTypeTable

@{
    ViewBag.Title = "Details";
}

<div class="card shadow mb-4">
    <div class="card-header py-3">
        <h6 class="m-0 font-weight-bold text-primary">User Types</h6>
    </div>
    <div class="card-body">
        <hr />
        <dl class="dl-horizontal">
            <dt>
                User Type
            </dt>

            <dd>
                @Html.DisplayFor(model => model.TypeName)
            </dd>

            <dt>
                Description
            </dt>

            <dd>
                @Html.DisplayFor(model => model.Description)
            </dd>

        </dl>
        @Html.ActionLink("Edit", "Edit", new { id = Model.UserTypeID }, new { @class = "btn btn-primary" })
        @Html.ActionLink("Back to List", "Index", null, new { @class = "btn btn-default" })
   
    </div>

</div>